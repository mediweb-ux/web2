# Environment Variables Template
# Copy this file to .env for local development

# Application Configuration
PUBLIC_APP_NAME="MediWeb Solutions"
PUBLIC_APP_VERSION="1.0.0"
PUBLIC_APP_URL="http://localhost:5173"

# Build Configuration
NODE_ENV="development"
BUILD_TARGET="development"

# Contact Form Configuration (if using external service)
# PUBLIC_CONTACT_FORM_ENDPOINT=""
# CONTACT_FORM_API_KEY=""

# Analytics Configuration (optional)
# PUBLIC_GOOGLE_ANALYTICS_ID=""
# PUBLIC_PLAUSIBLE_DOMAIN=""

# SEO Configuration
PUBLIC_SITE_NAME="MediWeb Solutions"
PUBLIC_SITE_DESCRIPTION="Moderne webutvikling, legetjenester og kursvirksomhet"
PUBLIC_SITE_KEYWORDS="web development,medical services,courses,agency"

# Social Media Configuration
PUBLIC_TWITTER_HANDLE="@agency"
PUBLIC_FACEBOOK_PAGE=""
PUBLIC_LINKEDIN_PAGE=""

# Performance Configuration
PUBLIC_ENABLE_SERVICE_WORKER="false"
PUBLIC_ENABLE_OFFLINE_MODE="false"